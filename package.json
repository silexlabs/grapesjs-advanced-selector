{
  "name": "@silexlabs/grapesjs-advanced-selector",
  "version": "1.0.0",
  "description": "Grapesjs Advanced Selector",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/@silexlabs/grapesjs-advanced-selector.git"
  },
  "scripts": {
    "start": "grapesjs-cli serve",
    "dev": "grapesjs-cli serve & http-server -p 8081 ../../node_modules/grapesjs",
    "build": "npm run lint && grapesjs-cli build --patch=false",
    "bump": "npm version patch -m 'Bump v%s'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "keywords": [
    "grapesjs",
    "plugin"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.13.0",
    "grapesjs": "^0.21.12",
    "grapesjs-cli": "^4.1.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "license": "MIT",
  "contributors": [
    "Alex Hoyau (https://github.com/lexoyo)"
  ],
  "dependencies": {
    "@silexlabs/expression-input": "^0.0.23",
    "lit": "^3.1.4"
  }
}
